  <meta name="twitter:site" content="@psionicalch">
  <meta name="twitter:creator" content="@psionicalch">

  <script type="application/ld+json">
    {
      "@context": "https://schema.org",
      "@type": "ItemList",
      "name": "Liked Tutorials",
      "description": "A list of tutorials liked by the user on PsionicAlch for personalized learning reference and skill development.",
