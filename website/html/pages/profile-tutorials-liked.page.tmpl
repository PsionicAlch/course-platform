        {
          "@type": "CreativeWork",
          "name": "Sample Liked Tutorial 1",
          "url": "https://www.psionicalch.com/tutorial/sample-liked-tutorial-1",
          "description": "Overview of Sample Liked Tutorial 1, a favorite tutorial for continued learning.",
          "educationalLevel": "Intermediate",
          "inLanguage": "English"
        },
        {
          "@type": "CreativeWork",
          "name": "Sample Liked Tutorial 2",
          "url": "https://www.psionicalch.com/tutorial/sample-liked-tutorial-2",
