{{ template "base" .}}

{{ define "meta-tags" }}
  <!-- TODO: Set up banner images -->
  <meta name="robots" content="noindex, nofollow">
  <meta name="description" content="Browse all the Golang tutorials you've liked on PsionicAlch. Keep track of your favorite content and continue building practical skills.">
  <meta name="keywords" content="Golang tutorials, liked tutorials, favorite tutorials, PsionicAlch profile, Golang learning resources, practical Golang applications, {User-Specific Keywords}">

  <meta property="og:title" content="Your Liked Tutorials | PsionicAlch">
  <meta property="og:description" content="Access all your liked tutorials in one place on PsionicAlch. Continue advancing your Golang expertise with content you enjoy.">
  <meta property="og:image" content="https://www.psionicalch.com/path-to-default-liked-banner.jpg">
