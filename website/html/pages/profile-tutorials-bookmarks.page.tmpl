{{ template "base" .}}

{{ define "meta-tags" }}
  <!-- TODO: Set up banner images -->
  <meta name="robots" content="noindex, nofollow">
  <meta name="description" content="Quickly access all the tutorials you've bookmarked on PsionicAlch. Continue building your Golang knowledge with tutorials saved for easy reference.">
  <meta name="keywords" content="Golang tutorials, bookmarked tutorials, saved tutorials, PsionicAlch profile, Golang learning resources, practical Golang applications, {User-Specific Keywords}">

  <meta property="og:title" content="Your Bookmarked Tutorials | PsionicAlch">
  <meta property="og:description" content="Access all your bookmarked tutorials in one place on PsionicAlch. Perfect for continuing your Golang journey.">
  <meta property="og:image" content="https://www.psionicalch.com/path-to-default-bookmarked-banner.jpg">
  <meta property="og:url" content="https://www.psionicalch.com/profile/tutorials/bookmarked">
  <meta property="og:type" content="website">
  <meta property="og:site_name" content="PsionicAlch">

  <meta name="twitter:card" content="summary">
  <meta name="twitter:title" content="Your Bookmarked Tutorials | PsionicAlch">
  <meta name="twitter:description" content="Quickly access all the Golang tutorials you've bookmarked on PsionicAlch. Stay on track with practical learning.">
  <meta name="twitter:image" content="https://www.psionicalch.com/path-to-default-bookmarked-banner.jpg">
  <meta name="twitter:site" content="@psionicalch">
  <meta name="twitter:creator" content="@psionicalch">

  <script type="application/ld+json">
    {
      "@context": "https://schema.org",
      "@type": "ItemList",
      "name": "Bookmarked Tutorials",
      "description": "A list of tutorials bookmarked by the user on PsionicAlch for easy access and continuous learning.",
      "itemListElement": [
        {
          "@type": "CreativeWork",
          "name": "Sample Tutorial 1",
          "url": "https://www.psionicalch.com/tutorial/sample-tutorial-1",
          "description": "Overview of Sample Tutorial 1, bookmarked for quick access.",
          "educationalLevel": "Intermediate",
          "inLanguage": "English"
        },
        {
          "@type": "CreativeWork",
          "name": "Sample Tutorial 2",
          "url": "https://www.psionicalch.com/tutorial/sample-tutorial-2",
          "description": "Overview of Sample Tutorial 2, bookmarked for easy learning reference.",
          "educationalLevel": "Intermediate",
          "inLanguage": "English"
        }
        // Continue with each bookmarked tutorial dynamically
      ]
    }
  </script>
{{ end }}

{{ define "stylesheets" }}
