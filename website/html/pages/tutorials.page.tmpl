{{ template "base" .}}

{{ define "meta-tags" }}
  <!-- TODO: Set up banner images -->
  <meta name="description" content="Explore a wide range of practical Golang tutorials designed to help developers build real-world applications. Gain hands-on skills to advance your portfolio and career.">
  <meta name="keywords" content="Golang tutorials, Golang programming, real-world Golang applications, Golang projects, Golang development, practical Golang skills, PsionicAlch tutorials">

  <!-- Open Graph (OG) Tags for Tutorials Page -->
  <meta property="og:title" content="Golang Tutorials | Practical, Real-World Applications | PsionicAlch">
  <meta property="og:description" content="Browse through our collection of Golang tutorials focused on building real-world applications. Perfect for developers looking to enhance their skills with hands-on projects.">
  <meta property="og:image" content="https://www.psionicalch.com/path-to-tutorials-banner.jpg">
  <meta property="og:url" content="https://www.psionicalch.com/tutorials">
  <meta property="og:type" content="website">
  <meta property="og:site_name" content="PsionicAlch">

